      headTitleBenefits={headTitleBenefits}
      titleBenefite1={titleBenefite1}
      descriptionBenefite1={descriptionBenefite1}
      titleBenefite2={titleBenefite2}
      descriptionBenefite2={descriptionBenefite2}
      titleBenefite3={titleBenefite3}
      descriptionBenefite3={descriptionBenefite3}
      CTA={CTA}
      imageBenefits={imageBenefits}
      imageAltBenefits={imageAltBenefits}
