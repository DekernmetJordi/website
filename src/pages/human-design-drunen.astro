  description="Op deze pagina is alles te vinden over de Human Design reading bij De kern met Jordi. Zit in Waalwijk dus bijna naast Drunen. Sessie kan face-to-face of online."
>
  <!-- <Section> -->
  <PaddingTopForPages>
    <HeroBannerForPages
      h1={targetKeyword}
      description={description}
      cta={cta}
      image={image}
      imageAlt={imageAlt}
    />
    <FeaturesForPages
      headTitleFeatures={headTitleFeatures}
      titleFeature1={titleFeature1}
      descriptionFeature1={descriptionFeature1}
      titleFeature2={titleFeature2}
      descriptionFeature2={descriptionFeature2}
      titleFeature3={titleFeature3}
      descriptionFeature3={descriptionFeature3}
    />
