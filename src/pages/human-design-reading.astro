      headTitleFeatures={headTitleFeatures}
      titleFeature1={titleFeature1}
      descriptionFeature1={descriptionFeature1}
      titleFeature2={titleFeature2}
      descriptionFeature2={descriptionFeature2}
      titleFeature3={titleFeature3}
      descriptionFeature3={descriptionFeature3}
    />
    <BenefitsForPages
      headTitleBenefits={headTitleBenefits}
      titleBenefite1={titleBenefite1}
      descriptionBenefite1={descriptionBenefite1}
      titleBenefite2={titleBenefite2}
      descriptionBenefite2={descriptionBenefite2}
      titleBenefite3={titleBenefite3}
      descriptionBenefite3={descriptionBenefite3}
    />
    <AboutMeForPages
      titleAboutMe={titleAboutMe}
      descriptionAboutMe1={descriptionAboutMe1}
      descriptionAboutMe2={descriptionAboutMe2}
      descriptionAboutMe3={descriptionAboutMe3}
      descriptionAboutMe4={descriptionAboutMe4}
      descriptionAboutMe5={descriptionAboutMe5}
      descriptionAboutMe6={descriptionAboutMe6}
