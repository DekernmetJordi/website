      checkMark2={checkMark2}
      checkMark3={checkMark3}
      checkMark4={checkMark4}
      priceProduct1={priceProduct1}
      discountProduct1={discountProduct1}
      ctaProduct1={ctaProduct1}
      titleProduct2={titleProduct2}
      descriptionProduct2={descriptionProduct2}
      checkMark5={checkMark5}
      checkMark6={checkMark6}
      checkMark7={checkMark7}
      checkMark8={checkMark8}
